<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Genix.Web</title>


    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/css/registerform.css" rel="stylesheet" />

    <link href="~/fontawesome-5.13.0/css/all.css" rel="stylesheet" />
    <link href="~/fontawesome-5.13.0/css/fontawesome.min.css" rel="stylesheet" />
    <link href="~/fontawesome-5.13.0/css/all.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        @{
            await Html.RenderPartialAsync("_BannerPartial");
        }
    </header>
    <div class="container-fluid">
        <main role="main" class="pb-3">
            @{ 
                await Html.RenderPartialAsync("_NotificationPartial");
            }
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - Genix.Web - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>
    <script src="~/fontawesome-5.13.0/js/all.js" asp-append-version="true"></script>
    <script src="~/fontawesome-5.13.0/js/all.min.js"></script>
    <script src="~/fontawesome-5.13.0/js/fontawesome.min.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
